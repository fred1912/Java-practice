 * Created by fredzhao on 7/05/17.
 */
public class Box<T> {
    private T t;
    public void add (T t){
        this.t = t;
    }
    public T get(){
        return t;
    }
    public static void main(String args[]){
        Box<Integer> integerBox = new Box<Integer>();
        integerBox.add((Integer)2);
        System.out.println(integerBox.get());
    }
}
